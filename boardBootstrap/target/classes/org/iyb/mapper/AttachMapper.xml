<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="org.iyb.mapper.AttachMapper">

  	<!-- attach테이블에 insert -->
  	<insert id="insert">
  		insert into attach(uuid,uploadpath,filename,image,bno)
		values(#{uuid},#{uploadPath},#{fileName},#{image},#{bno});
  	</insert>
  	
  	<select id="fileList" resultType="org.iyb.domain.AttachFileDTO">
  		select*from attach where bno=#{bno};
  	</select>
</mapper>